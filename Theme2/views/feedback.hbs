<!DOCTYPE html>
{{>headinclude}}

<body class = "container-fluid">
	<div class="page-root">
	  {{>header}}
	  <div role="main" class="styles__layoutWrapper__nEkzc">
	  	 <div class="create-complaints">
	  	 	{{#if collegeName}}
	  			<div class="collg-name">College : {{collegeName}}</div>
	  		{{/if}}
	  		<div class="two-col">
		  	 	<div class="panel-group" id="accordion">
			  	 {{#each feedbackFields}}
		  	 		<div class="panel panel-default">
		  	 			<div class="panel-heading">
	                        <h4 class="panel-title">
	                            <a data-toggle="collapse" data-parent="#accordion" href="#collapse{{@index}}" aria-expanded="false">
									{{this.name}}
								</a>
	                        </h4>
	                    </div>
	                    <div id="collapse{{@index}}" class="panel-collapse collapse in">
	                        <div class="panel-body">
	                        	<fieldset class="smileys-wrapper">
							        <input type="radio" id="star3" name="smiley" value="sad" class="sad" />
							        <input type="radio" id="star2" name="smiley" value="neutral" class="neutral" />
							        <input type="radio" id="star1" name="smiley" value="happy" class="happy" />
							    </fieldset>
							    <textarea></textarea>
	                        </div>
	                    </div>
	                </div>
			  	 {{/each}}
		  	 	</div>
		  	 	<input type="submit" name="" value="submit">
	  	 	</div>
	  	 	<div class="two-col">
	  	 		<span>
	  	 			Oops! It looks like you don't have raised ant ticket here.
	  	 		</span>
	  	 	</div>
	  	</div>
	  </div>
	  {{>footer}}
	</div>
</body>

</html>
