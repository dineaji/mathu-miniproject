<!DOCTYPE html>
{{>headinclude}}

<body class = "container-fluid">
	<div class="page-root">
	  {{>header}}
	  <div role="main" class="styles__layoutWrapper__nEkzc">
	  	 <div class="create-complaints">
	  	 	<ul>
		  	 {{#each feedbackFields}}
		  	 	<li>
		  	 		<label>{{this.name}}</label>
		  	 		<fieldset class="rating">
				        <input type="radio" id="star5" name="rating" value="5" /><label for="star5" title="Excellent">5 stars</label>
				        <input type="radio" id="star4" name="rating" value="4" /><label for="star4" title="Good">4 stars</label>
				        <input type="radio" id="star3" name="rating" value="3" /><label for="star3" title="Fair">3 stars</label>
				        <input type="radio" id="star2" name="rating" value="2" /><label for="star2" title="Poor">2 stars</label>
				        <input type="radio" id="star1" name="rating" value="1" /><label for="star1" title="Awful">1 star</label>
				    </fieldset>
				    <textarea></textarea>
		  	 	</li>
		  	 {{/each}}
	  	 	</ul>
	  	 	<input type="submit" name="" value="submit">
	  	</div>
	  </div>
	  {{>footer}}
	</div>
</body>

</html>
